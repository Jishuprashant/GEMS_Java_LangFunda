package com.java;
import java.util.*;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int num = sc. nextInt();
		int[] array= new int[num];
		int count=num;
		
		for(int i =0; i<num; i++){
			array[i] = sc.nextInt();
		}
		for( int i = 0; i < num ; i++) {
			for(int j=0; j<i; j++) {
				if(array[i] == array[j]) {
					count--;
				}
			}
		}
		System.out.println(count);
		
		sc.close();

	}

}
